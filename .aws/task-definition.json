{
    "family": "myapp-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
      {
        "name": "myapp",
        "image": "<IMAGE_URI>",
        "portMappings": [
          {
            "containerPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ],
    "executionRoleArn": "arn:aws:iam::<tu-id>:role/ecsTaskExecutionRole"
  }
  